{
    "manifest_version": 3,
    "name": "Studocu Paywall Remover",
    "version": "0.0.0.1",
    "description": "Removes the paywall on Studocu.com documents!",
    "content_scripts": [
        {
            "css": [
                "styles/no_blur.css"
            ],
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "https://www.studocu.com/*/document/*"
            ]
        }
    ],
    "icons": {
        "64": "icons/icon.png"
    }
}